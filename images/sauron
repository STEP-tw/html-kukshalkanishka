    function abTest(){
        // An element is created and styled to test for ad blocking technology
        var ad = document.createElement('gaAdblockTestElement');
        // The element is given the class of "Adsense" since it is likely to be blocked by most ad blockers
        ad.className = 'AdSense';
        ad.style.display = 'block';
        ad.style.position = 'absolute';
        ad.style.top = '-1px';
        ad.style.height = '1px';
        // The element is appended to the body
        document.body.appendChild(ad);
        // If the element height equals 1, then the element was not interfered with and ad blocking is not present.
        if (ad.clientHeight == 1) {
            var isAdBlockEnabled = false;
        }
        else {
            var isAdBlockEnabled = true;
        }
        // The element is removed from the body
        document.body.removeChild(ad);
        // The value is returned for the variable
        return isAdBlockEnabled;
    }

