<source media="(min-width: 481px)" sizes="737px" data-srcset="https://static3.srcdn.com/wordpress/wp-content/uploads/2017/09/Lord-of-the-Rings-Movie-Sauron-Eye-1.jpg?q=50&amp;fit=crop&amp;w=737 737w"/>
<source media="(min-width: 0px) and (-webkit-min-device-pixel-ratio: 1.25), (min-width: 0px) and (min-resolution: 120dpi)" sizes="450px" data-srcset="https://static3.srcdn.com/wordpress/wp-content/uploads/2017/09/Lord-of-the-Rings-Movie-Sauron-Eye-1.jpg?q=50&amp;fit=crop&amp;w=450&dpr=1.5 675w"/>
